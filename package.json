{
	"name": "quick-threejs",
	"version": "0.0.0",
	"description": "Three,js quick starters",
	"private": true,
	"scripts": {
		"dev": "turbo dev",
		"build": "turbo build",
		"test": "turbo test",
		"test:e2e": "turbo test:e2e",
		"lint": "eslint . && turbo lint",
		"format": "prettier --write \"**/*.{js,mjs,ts,tsx,json,md}\"",
		"release": "pnpm changeset publish && git push --follow-tags"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.1.0",
		"@eslint/js": "^9.6.0",
		"@quick-threejs/config": "workspace:*",
		"globals": "^15.8.0",
		"prettier": "^3.3.2",
		"turbo": "latest"
	},
	"dependencies": {
		"@changesets/cli": "^2.27.7"
	},
	"packageManager": "pnpm@8.15.6",
	"engines": {
		"node": ">=18"
	}
}
