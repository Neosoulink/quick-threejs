{
	"name": "quick-threejs",
	"version": "0.1.30",
	"description": "Quick threejs helper",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Neosoulink/quick-threejs.git"
	},
	"keywords": [
		"threejs",
		"js",
		"ts"
	],
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"author": "Neosoulink",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Neosoulink/quick-threejs/issues"
	},
	"homepage": "https://github.com/Neosoulink/quick-threejs#readme",
	"scripts": {
		"test": "jest",
		"build": "tsc",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint --ext .ts,.js ./src",
		"prepare": "npm run build",
		"prepublishOnly": "npm test && npm run lint",
		"preversion": "npm run lint",
		"version": "npm run format && git add -A src",
		"postversion": "git push && git push --tags",
		"release": "npm run prepare && np"
	},
	"dependencies": {
		"events": "^3.3.0",
		"three": "^0.159.0",
		"stats.js": "^0.17.0",
		"lil-gui": "^0.19.1"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.22.4",
		"@types/jest": "^29.5.3",
		"@types/stats.js": "^0.17.0",
		"@types/three": "^0.159.0",
		"@typescript-eslint/eslint-plugin": "^6.1.0",
		"@typescript-eslint/parser": "^6.1.0",
		"babel-jest": "^29.5.0",
		"eslint": "^8.45.0",
		"jest": "^29.5.0",
		"prettier": "^3.1.0",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	},
	"files": [
		"lib/**/*"
	],
	"engines": {
		"node": ">=17.0.0"
	}
}
