{
	"name": "quick-threejs",
	"description": "Three,js quick starters",
	"private": true,
	"scripts": {
		"dev": "turbo dev",
		"build": "turbo build",
		"test": "turbo test",
		"test:e2e": "turbo test:e2e",
		"lint": "turbo lint && eslint",
		"format": "prettier --ignore-unknown --write \"**/*.{js,jsx,cjs,mjs,ts,tsx,html,json,css,scss,md,mdx}\"",
		"release": "pnpm changeset publish && git push --follow-tags",
		"prepare": "husky"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@eslint/eslintrc": "^3.1.0",
		"@eslint/js": "^9.6.0",
		"@quick-threejs/config": "workspace:*",
		"globals": "^15.8.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7",
		"prettier": "^3.2.5",
		"turbo": "latest"
	},
	"dependencies": {
		"@changesets/cli": "^2.27.7"
	},
	"packageManager": "pnpm@8.15.6",
	"engines": {
		"node": ">=18"
	}
}
