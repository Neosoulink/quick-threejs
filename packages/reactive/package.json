{
	"name": "@quick-threejs/reactive",
	"version": "0.1.4",
	"description": "Reactively Three.js quick starter ⚡️",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Neosoulink/quick-threejs.git"
	},
	"keywords": [
		"threejs",
		"js",
		"ts"
	],
	"files": [
		"dist/**/*"
	],
	"main": "./dist/main.js",
	"types": "./dist/main.d.ts",
	"author": "Neosoulink",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Neosoulink/quick-threejs/issues"
	},
	"homepage": "https://github.com/Neosoulink/quick-threejs#readme",
	"scripts": {
		"prepare": "npm run build",
		"dev": "vite --host",
		"test": "jest",
		"build": "tsc",
		"prepublishOnly": "npm test && npm run lint",
		"preversion": "npm run lint",
		"version": "npm run format && git add -A src",
		"postversion": "git push && git push --tags",
		"release": "npm run prepare && np --branch main",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint --ext .ts,.js ./src"
	},
	"dependencies": {
		"@quick-threejs/utils": "workspace:*",
		"threads": "^1.7.0",
		"three": "^0.166.0"
	},
	"devDependencies": {
		"@quick-threejs/config": "workspace:*",
		"@types/events": "^3.0.3",
		"@types/jest": "^29.5.3",
		"@types/node": "^20.14.2",
		"@types/stats.js": "^0.17.0",
		"@types/three": "^0.166.0",
		"babel-jest": "^29.5.0",
		"events": "^3.3.0",
		"jest": "^29.5.0",
		"lil-gui": "^0.19.1",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1",
		"stats.js": "^0.17.0",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.2",
		"tsyringe": "^4.8.0",
		"vite": "^5.2.12"
	},
	"engines": {
		"node": ">=17.0.0"
	}
}
