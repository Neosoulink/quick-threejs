{
	"name": "@quick-threejs/reactive",
	"version": "0.1.21",
	"description": "Reactively quick start a three.js app ⚡️",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Neosoulink/quick-threejs.git"
	},
	"keywords": [
		"threejs",
		"js",
		"ts"
	],
	"main": "./dist/main.js",
	"module": "./dist/main.mjs",
	"types": "dist/**/*",
	"files": [
		"dist/**/*"
	],
	"exports": {
		".": {
			"types": "./dist/main.d.ts",
			"node": "./dist/main.js",
			"default": "./dist/main.mjs"
		},
		"./worker": {
			"types": "./dist/main.worker.d.ts",
			"node": "./dist/main.worker.js",
			"default": "./dist/main.worker.mjs"
		}
	},
	"author": "Neosoulink",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Neosoulink/quick-threejs/issues"
	},
	"homepage": "https://github.com/Neosoulink/quick-threejs#readme",
	"scripts": {
		"build": "tsup",
		"dev": "pnpm build --watch",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint --max-warnings=0",
		"prepare": "pnpm run lint && pnpm run build",
		"test": "jest --passWithNoTests"
	},
	"dependencies": {
		"@quick-threejs/utils": "workspace:*",
		"rxjs": "^7.8.1",
		"threads": "^1.7.0",
		"three": "^0.169.0",
		"stats.js": "^0.17.0"
	},
	"devDependencies": {
		"@quick-threejs/config": "workspace:*",
		"@types/events": "^3.0.3",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.9.0",
		"@types/stats.js": "^0.17.3",
		"@types/three": "^0.169.0",
		"babel-jest": "^29.7.0",
		"events": "^3.3.0",
		"jest": "^29.7.0",
		"reflect-metadata": "^0.2.2",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"tsyringe": "^4.8.0"
	}
}
