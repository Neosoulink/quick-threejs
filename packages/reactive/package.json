{
	"name": "@quick-threejs/reactive",
	"version": "1.0.0",
	"description": "Reactively Three.js quick starter ⚡️",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Neosoulink/quick-threejs.git"
	},
	"keywords": [
		"threejs",
		"js",
		"ts"
	],
	"type": "module",
	"main": "./dist/main.js",
	"module": "./dist/main.js",
	"types": "dist/**/*",
	"files": [
		"dist/**/*"
	],
	"exports": {
		".": {
			"types": "./dist/main.d.ts",
			"import": "./dist/main.js"
		},
		"./worker": {
			"types": "./dist/main.worker.d.ts",
			"import": "./dist/main.worker.js"
		}
	},
	"author": "Neosoulink",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Neosoulink/quick-threejs/issues"
	},
	"homepage": "https://github.com/Neosoulink/quick-threejs#readme",
	"scripts": {
		"build": "tsc",
		"dev": "vite --host",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint",
		"prepare": "pnpm run lint && pnpm run build",
		"test": "jest --passWithNoTests"
	},
	"dependencies": {
		"@quick-threejs/utils": "workspace:*",
		"threads": "^1.7.0",
		"three": "^0.165.0"
	},
	"devDependencies": {
		"@quick-threejs/config": "workspace:*",
		"@types/events": "^3.0.3",
		"@types/jest": "^29.5.3",
		"@types/node": "^20.14.2",
		"@types/stats.js": "^0.17.0",
		"@types/three": "^0.165.0",
		"babel-jest": "^29.5.0",
		"events": "^3.3.0",
		"jest": "^29.5.0",
		"lil-gui": "^0.19.1",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1",
		"stats.js": "^0.17.0",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.2",
		"tsyringe": "^4.8.0",
		"vite": "^5.2.12"
	}
}
